{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@layouts/*": ["src/layouts/*"],
      "@views/*": ["src/views/*"],
      "@pages/*": ["src/pages/*"],
      "@services/*":["src/services/*"],
      "@hook/*":["src/hook/*"],
      "@store/*":["src/store/*"],
    }
  }
}